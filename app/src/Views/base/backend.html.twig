{% block javascripts %}
    <!-- Bootstrap -->
    <script src="{{ base_path }}/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <!-- AdminLTE App -->
    <script src="{{ base_path }}/js/app.js" type="text/javascript"></script>

    <script type="text/javascript">
        function getColor( num ) {

            var rColor = [
                "#F7464A",
                "#46BFBD",
                "#FDB45C",
                "#949FB1",
                "#4D5360",
                "#B48EAD",
                //highlight
                "#FF5A5E",
                "#5AD3D1",
                "#FFC870",
                "#A8B3C5",
                "#616774",
                "#C69CBE",
            ];

            var color = '';
            if (num > rColor.length) {
                while (!color.match(/(#[c-e].)([e-f][a-f])([9-c].)/)) {
                    color = '#' + Math.floor(Math.random() * (Math.pow(16,6))).toString(16);
                }
            }
            else{
                color = rColor[num];
            }

            return color;
        }
    </script>

{% endblock %}